# {{ ansible_managed }}
# TinyAuth environment variables - generated from Ansible Vault

APP_URL={{ tinyauth_app_url }}
SECRET={{ tinyauth_secret }}
LOG_LEVEL={{ tinyauth_log_level }}
COOKIE_SECURE={{ tinyauth_cookie_secure }}
COOKIE_DOMAIN={{ tinyauth_cookie_domain }}
SESSION_DURATION={{ tinyauth_session_duration }}
OAUTH_AUTO_REDIRECT={{ tinyauth_oauth_auto_redirect }}

# PocketID OAuth
OAUTH_POCKETID_NAME={{ tinyauth_pocketid_name }}
OAUTH_POCKETID_CLIENT_ID={{ tinyauth_pocketid_client_id }}
OAUTH_POCKETID_CLIENT_SECRET={{ tinyauth_pocketid_client_secret }}
OAUTH_POCKETID_AUTH_URL={{ tinyauth_pocketid_auth_url }}
OAUTH_POCKETID_TOKEN_URL={{ tinyauth_pocketid_token_url }}
OAUTH_POCKETID_USER_INFO_URL={{ tinyauth_pocketid_user_info_url }}
OAUTH_POCKETID_REDIRECT_URL={{ tinyauth_pocketid_redirect_url }}
OAUTH_POCKETID_SCOPES={{ tinyauth_pocketid_scopes }}
