# {{ ansible_managed }}
port: 7880
bind_addresses:
  - "0.0.0.0"
rtc:
  tcp_port: 7881
  port_range_start: {{ livekit_rtc_port_start }}
  port_range_end: {{ livekit_rtc_port_end }}
  use_external_ip: true
  enable_loopback_candidate: false
keys:
  {{ livekit_api_key }}: {{ livekit_api_secret }}
room:
  auto_create: false
logging:
  level: info
turn:
  enabled: false
